pragma solidity >=0.6.12 <0.9.0;

contract AdvanceStorage{
  int256 private intVariable;
  uint8 private uintVariable;
  string private stringvariable;
  address private addressVarible;
  function getIntVariable() public view returns (int256){
    return intVariable;
  }
  function  setIntvariable(int256 _value) public returns (int256){
    intVariable = _value;
    return intVariable;

  }
  function getUintVariable() public view returns (uint8) {
    return uintVariable;
  }
  function setUintVariable(uint8 _value) public returns (uint8){
    uintVariable = _value;
    return uintVariable;
  }
  function getStringVariable() public view returns (string memory){
    return stringvariable;
  }
  function setStringVariable(string memory _value) public returns (string memory){
    stringvariable = _value;
    return stringvariable;
  }
  function getAddressVariable() public view returns (address){
    return addressVarible;
  }
  function setAddressVariable(address _value) public returns (address){
    addressVarible = _value;
    return addressVarible;
  }
}
      
